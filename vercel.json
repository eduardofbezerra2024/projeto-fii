{
  "crons": [
    {
      "path": "/api/cron_master",
      "schedule": "0 11 * * *"
    },
    {
      "path": "/api/cron_alerts",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/daily-news",
      "schedule": "0 11 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ]
}